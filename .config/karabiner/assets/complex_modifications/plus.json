{
  "title": "Change 9 to option",
  "rules": [
    {
      "description": "Change 9 to left_option if pressed with other keys (Post 9 when pressed alone)",
      "manipulators": [
        {
          "from": {
            "key_code": "9",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_option",
              "lazy": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "9"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
